<!DOCTYPE html>
<html>
{{> head.hbs}}
<body>
  <main>
    <img src="logo.png" alt="Brocessing">
    <h1>{{title}}</h1>
    <ul>
      {{#each links}}
        <li class="projects">
          <a class="links" href="{{url}}" target=_blank>{{name}}</a>
          <caption>{{caption}}</caption></li>
          <p class="technos">{{technos}}</p>
      {{/each}}
    </ul>
  </main>
  <script src="{{compiler.hash}}.js"></script>
</body>
</html>